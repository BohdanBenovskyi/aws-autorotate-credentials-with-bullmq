generator client {
  provider = "prisma-client-js"
  output = "../src/database/generated/client"
  binaryTargets = ["native", "darwin", "darwin-arm64"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model configuration {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  mapping  Json
}